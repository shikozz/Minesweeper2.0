<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:System="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="BackgroundColor">White</Color>
    <Color x:Key="ForegroundColor">Black</Color>
    <Color x:Key="ButtonBackgroundColor">LightGray</Color>
    <Color x:Key="ButtonForegroundColor">Black</Color>
    <Color x:Key="ButtonHoverBackgroundColor">LightBlue</Color>
    <Color x:Key="ButtonHoverForegroundColor">Black</Color>
    <Color x:Key="ButtonFlaggedBackgroundColor">LightYellow</Color>
    <Color x:Key="ButtonMineBackgroundColor">Red</Color>
    <Color x:Key="ButtonAdjacentBackgroundColor">GhostWhite</Color>
    <Color x:Key="ButtonRevealedBackgroundColor">WhiteSmoke</Color>
    <Color x:Key="ButtonSonarBackgroundColor">LightGreen</Color>
    <Color x:Key="ScoreLabelBackgroundColor">White</Color>
    <Color x:Key="ScoreLabelForegroundColor">Black</Color>
    <Color x:Key="ScoreLabelWinBackgroundColor">LightGreen</Color>
    <Color x:Key="ScoreLabelWinForegroundColor">Green</Color>
    <Color x:Key="ScoreLabelLoseBackgroundColor">Pink</Color>
    <Color x:Key="ScoreLabelLoseForegroundColor">Red</Color>
    <Color x:Key="ButtonHoverBoostActiveBackgroundColor">Yellow</Color>
    <Color x:Key="glowEffectColor">Transparent</Color>
    <FontFamily x:Key="fontFamily">Courier New</FontFamily>
    <System:Boolean x:Key="IsGlowEnabled">True</System:Boolean>
    
    <SolidColorBrush x:Key="glowEffectBrush" Color="{StaticResource glowEffectColor}"/>
    <SolidColorBrush x:Key="ButtonHoverBoostActiveBackgroundBrush" Color="{StaticResource ButtonHoverBoostActiveBackgroundColor}"/>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource ForegroundColor}"/>
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="{StaticResource ButtonBackgroundColor}"/>
    <SolidColorBrush x:Key="ButtonForegroundBrush" Color="{StaticResource ButtonForegroundColor}"/>
    <SolidColorBrush x:Key="ButtonHoverBackgroundBrush" Color="{StaticResource ButtonHoverBackgroundColor}"/>
    <SolidColorBrush x:Key="ButtonHoverForegroundBrush" Color="{StaticResource ButtonHoverForegroundColor}"/>
    <SolidColorBrush x:Key="ButtonFlaggedBackgroundBrush" Color="{StaticResource ButtonFlaggedBackgroundColor}"/>
    <SolidColorBrush x:Key="ButtonMineBackgroundBrush" Color="{StaticResource ButtonMineBackgroundColor}"/>
    <SolidColorBrush x:Key="ButtonAdjacentBackgroundBrush" Color="{StaticResource ButtonAdjacentBackgroundColor}"/>
    <SolidColorBrush x:Key="ButtonRevealedBackgroundBrush" Color="{StaticResource ButtonRevealedBackgroundColor}"/>
    <SolidColorBrush x:Key="ButtonSonarBackgroundBrush" Color="{StaticResource ButtonSonarBackgroundColor}"/>
    <SolidColorBrush x:Key="ScoreLabelBackgroundBrush" Color="{StaticResource ScoreLabelBackgroundColor}"/>
    <SolidColorBrush x:Key="ScoreLabelForegroundBrush" Color="{StaticResource ScoreLabelForegroundColor}"/>
    <SolidColorBrush x:Key="ScoreLabelWinBackgroundBrush" Color="{StaticResource ScoreLabelWinBackgroundColor}"/>
    <SolidColorBrush x:Key="ScoreLabelWinForegroundBrush" Color="{StaticResource ScoreLabelWinForegroundColor}"/>
    <SolidColorBrush x:Key="ScoreLabelLoseBackgroundBrush" Color="{StaticResource ScoreLabelLoseBackgroundColor}"/>
    <SolidColorBrush x:Key="ScoreLabelLoseForegroundBrush" Color="{StaticResource ScoreLabelLoseForegroundColor}"/>
</ResourceDictionary>